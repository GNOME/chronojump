msgid "Excellent"
msgstr "Excelente"

#: ../src/runType.cs:238
msgid "Good"
msgstr "Bom"

#: ../src/runType.cs:240
msgid "Fair"
msgstr "Razoável"

#: ../src/runType.cs:241
